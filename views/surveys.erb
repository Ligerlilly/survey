<h2>Make Surveys</h2>

<% if Survey.any? %>
  <ul>
    <% Survey.all.each do |survey| %>
      <li><a href="/surveys/<%= survey.id %>"><%=survey.title %></a> | <a href="/delete/<%= survey.id %>" class='delete'>delete</a></li>
    <% end %>
  </ul>

<% else %>
  <p> There are no surveys!</p>
<% end %>

    <a href="/surveys/new">Add Survey</a> |
